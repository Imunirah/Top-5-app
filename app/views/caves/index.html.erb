<%# <h1>Caves#index</h1>
<p>Find me in app/views/caves/index.html.erb</p> %>


<h1>Top 5 Caves</h1>


<table class="table table-striped">
  <tr>
    <th></th>
    <th>Caves</th>
    <th>Rate</th>
    <th>Location</th>
 
  </tr>

  <% @caves.each do |cafe| %>
      <tr>
      <td><%= image_tag(cafe.img, size: "150x110", alt: "Resturant logo") %></td>
        <td><%= link_to cafe.name, cafe_path(cafe) %></td>
        <td><%= cafe.rate%></td>
        <td><%= cafe.location %></td>
      </tr>
  <% end %>
</table>